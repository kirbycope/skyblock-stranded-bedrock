{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.teleport_minecart": {
			"states": {
				"default": {
					"transitions": [
						{
							"teleport": "query.get_equipped_item_name == 'skyblock_star'"
						},
						{
							"no-teleport": "query.get_equipped_item_name != 'skyblock_star'"
						}
					]
				},
				"teleport": {
					"on_entry": [
						"/function menus/chest-menu-open"
					],
					"transitions": [
						{
							"default": "query.is_alive"
						}
					]
				},
				"no-teleport": {
					"on_entry": [
						"/teleport @e[type=cope:invisible_chest_minecart] 0 -64 0"
					],
					"transitions": [
						{
							"default": "query.is_alive"
						}
					]
				}
			}
		}
	}
}